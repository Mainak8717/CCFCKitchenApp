<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:CCFCKitchenApp" x:Class="CCFCKitchenApp.CCFCKitchenAppPage">
	<StackLayout Padding="20,20,20,20" Spacing="10" HorizontalOptions="Center" VerticalOptions="Center">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="300" />
				<RowDefinition Height="300" />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="300" />
				<ColumnDefinition Width="300" />
			</Grid.ColumnDefinitions>
			<Image x:Name ="drinksItemImg" Source="drinksImg.jpeg" Grid.Row="0" Grid.Column="0" />
			<Image x:Name = "foodItemImg" Source="foodImg.jpeg" Grid.Row="0" Grid.Column="1" />
			<Image x:Name = "dessertsItemImg" Source="dessertsImg.jpeg" Grid.Row="1" Grid.Column="0" />
			<Image x:Name = "beveragesItemImg" Source="beveragesImg.jpeg" Grid.Row="1" Grid.Column="1" />

		</Grid>
	</StackLayout>
</ContentPage>